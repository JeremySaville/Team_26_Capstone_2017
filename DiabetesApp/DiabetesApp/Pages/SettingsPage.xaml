<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DiabetesApp.SettingsPage"
             Icon="ic_settings_white.png"
             Style="{StaticResource pageStyle}">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Settings" Style="{StaticResource titleStyle}"/>
            <Button x:Name="logout" Text="Log Out" Style="{StaticResource buttonStyle}" Clicked="onClick_logoutButton"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>