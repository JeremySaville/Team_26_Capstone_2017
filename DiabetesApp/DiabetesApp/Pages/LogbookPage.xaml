<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DiabetesApp.LogbookPage"
             Style="{StaticResource pageStyle}">
    <ContentPage.Content>
        <ScrollView>
            <StackLayout>
                <Label Text="Logbook" FontSize="Large" TextColor="Black" Margin="10" HorizontalOptions="Center"/>
                <Button Text="Create Entry" Clicked="onClick_createEntry" Style="{StaticResource buttonStyle}"/>
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>