<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DiabetesApp.Pages.Quiz12">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="If you are sick and cannot eat, you should..." FontSize="Large" FontAttributes="Bold"
                 HorizontalOptions="Center"
                 VerticalOptions="Center"
                 Margin="20"
                />
            <Label Text="If you are sick and your blood sugar is 17mmol/L, you should..." FontSize="Large" FontAttributes="Bold"
                 HorizontalOptions="Center"
                 VerticalOptions="Center"
                 Margin="20"
                />
            <Label Text="Which are the following are barriers commonly faced by young people with diabetes?" FontSize="Large" FontAttributes="Bold"
                 HorizontalOptions="Center"
                 VerticalOptions="Center"
                 Margin="20"
                />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>